{
    "name": "cereal-al-db",
    "version": "0.1.0",
    "homepage": "/db",
    "private": true,
    "dependencies": {
        "@types/node": "^18.11.7",
        "@types/react": "^18.0.24",
        "@types/react-dom": "^18.0.8",
        "bootstrap": "^5.2.2",
        "eslint": "^8.26.0",
        "eslint-config-next": "^13.0.0",
        "next": "^13.0.0",
        "react": "^18.2.0",
        "react-bootstrap": "^2.5.0",
        "react-dom": "^18.2.0",
        "typescript": "^4.8.4"
    },
    "scripts": {
        "dev": "next dev",
        "build": "next build && next export",
        "start": "next start",
        "lint": "next lint"
    },
    "eslintConfig": {
        "extends": "next",
        "rules": {
            "@next/next/no-img-element": "off"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^3.4.0",
        "prettier": "^2.7.1"
    },
    "prettier": {
        "tabWidth": 4,
        "importOrder": [
            "<THIRD_PARTY_MODULES>",
            "^[./].*(?<!css)$",
            "^[\\w](.*).css$",
            "^[./](.*).css$",
            "^[./]"
        ],
        "importOrderSeparation": true
    }
}
